[ITEMDEF i_daily_quest_clock] 
ID=i_clock
TYPE=t_script
NAME=Daily Quest Clock

ON=@CREATE
TIMERF 1, f_pick_daily_quest
ATTR=08010
MOREX=0
MOREY=0
MOREZ=1
TIMER=60

ON=@TIMER 
MOREX=<EVAL <MOREX>+1> // A minute has passed
IF (<EVAL <MOREX>>==60)
	MOREX=0
	MOREY=<EVAL <MOREY>+1> // An hour has passed
	IF (<EVAL <MOREY>>==24) // A day has passed
		MOREX=0
		MOREY=0
		MOREZ=<EVAL <MOREZ>+1>
		TAG.DQ_MONSTERS_PICKED=
		TAG.ROUND=0
		f_pick_daily_quest // Generate a new Daily Quest
	ENDIF
ENDIF
TIMER=60
RETURN 1

[FUNCTION f_pick_daily_quest]
IF !(<EVAL <ARGS>>==1) // If the function was forced by itself and not by the clock, then it will assign a new monster number, up to the desired monster amount
	TAG.ROUND=<EVAL <TAG0.ROUND>+1>
ENDIF

