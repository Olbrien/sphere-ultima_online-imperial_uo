ON=51

IF (<ARGCHK[49]>==1)
	TAG.PLAYERS=1
ELSE
	IF (<EVAL <TAG0.ISPLAYER>>==0)
		TAG.PLAYERS=
	ENDIF
ENDIF

FOR 1 48
	IF !(<EVAL <LOCAL._FOR>>==7)
		IF (<ARGCHK[<EVAL <LOCAL._FOR>>]>==1)
			LOCAL.ARGCHK +=1
		ENDIF
	ENDIF
ENDFOR

IF !(<LOCAL.ARGCHK>)
	SRC.SYSMESSAGE Please select which skills you want the Temporary Skills Robe to increase and the respective skill amount.
	DIALOG d_skill_robe
	RETURN 1
ELSE
	IF (<EVAL <TAG0.PLAYERS>>==1)
		IF (<EVAL <LOCAL.ARGCHK>> > 10)
			SRC.SYSMESSAGE You can only select up to 10 skills to be increased with the Temporary Skills Robe.
			DIALOG d_skill_robe
			RETURN 1
		ENDIF
	ENDIF
ENDIF
	
FOR 1 48
	IF !(<EVAL <LOCAL._FOR>>==7)
		IF !(<EVAL <ARGTXT[<EVAL <LOCAL._FOR>>]>>==0)
			LOCAL.ARGTXT +=1
		ENDIF
	ENDIF
ENDFOR

IF !(<LOCAL.ARGTXT>)
	SRC.SYSMESSAGE Please select which skills you want the Temporary Skills Robe to increase and the respective skill amount.
	DIALOG d_skill_robe
	RETURN 1
ELSE
	IF (<EVAL <TAG0.PLAYERS>>==1)
		FOR 1 48
			IF !(<EVAL <LOCAL._FOR>>==7)
				IF (<EVAL <ARGTXT[<EVAL <LOCAL._FOR>>]>> > 1000)
					SRC.SYSMESSAGE You can not choose a skill to be set to more than 100%.
					DIALOG d_skill_robe
					RETURN 1
				ENDIF
			ENDIF
		ENDFOR
	ENDIF
ENDIF

FOR 1 48
	TAG.SKILL<EVAL (<LOCAL._FOR>-1)>=
	IF !(<EVAL <LOCAL._FOR>>==7)
		IF !(<ARGCHK[<EVAL <LOCAL._FOR>>]>==0) && !(<EVAL <ARGTXT[<EVAL <LOCAL._FOR>>]>>==0)
			LOCAL.SUCCESS +=1
			TAG.SKILL<EVAL (<LOCAL._FOR>-1)>=<EVAL <ARGTXT[<EVAL <LOCAL._FOR>>]>>
		ENDIF
	ENDIF
ENDFOR

IF (<LOCAL.SUCCESS>)
	SRC.SYSMESSAGE Temporary Skills Robe settings successfully applied.
ELSE
	SRC.SYSMESSAGE Please select which skills you want the Temporary Skills Robe to increase and the respective skill amount.
	DIALOG d_skill_robe
	RETURN 1
ENDIF



