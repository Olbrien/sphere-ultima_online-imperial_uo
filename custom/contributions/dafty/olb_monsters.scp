[FUNCTION ctfstart]
IF (<SRC.FINDID.m_ctf_countdown.UID>)
	SRC.SYSMESSAGE There is already a Capture the Flag Event Countdown going on.
	RETURN 1
ENDIF

SERV.NEWITEM m_ctf_countdown
NEW.EQUIP
RETURN 1

[ITEMDEF m_ctf_countdown]
ID=i_memory
NAME=CTF Countdown
TYPE=t_eq_script

ON=@CREATE
TIMER=10

ON=@EQUIP
CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag Event will start in 30 seconds. Get ready.

ON=@TIMER
IF (<EVAL <MORE1>>==0)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag Event will start in 20 seconds. Get ready.
	MORE1=<EVAL <MORE1>+1>
	TIMER=10
ELSEIF (<EVAL <MORE1>>==1)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag Event will start in 10 seconds. Get ready.
	MORE1=<EVAL <MORE1>+1>
	TIMER=10
ELSEIF (<EVAL <MORE1>>==2)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag Event has started! Collect as many flags as you can within 10 minutes to be the winner.
	MORE1=<EVAL <MORE1>+1>
	TIMER=300
ELSEIF (<EVAL <MORE1>>==3)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag round ending in 5 minutes.
	MORE1=<EVAL <MORE1>+1>
	TIMER=270
ELSEIF (<EVAL <MORE1>>==4)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag round ending in 30 seconds.
	MORE1=<EVAL <MORE1>+1>
	TIMER=20
ELSEIF (<EVAL <MORE1>>==5)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag round ending in 10 seconds.
	MORE1=<EVAL <MORE1>+1>
	TIMER=10
ELSEIF (<EVAL <MORE1>>==6)
	CONT.REGION.ALLCLIENTS SYSMESSAGE @90 Capture the Flag round is now over and no more Flags can be collected.
	CONT.capturetheflag_finish
	REMOVE
ENDIF
RETURN 1

[FUNCTION capturetheflag_finish]
FORINSTANCES i_ctf_tile
		LOCAL.TILES=<EVAL <LOCAL.TILES>+1>
		LOCAL.COLOR_<EVAL <LOCAL.TILES>>=<COLOR>
		LOCAL.TEAM_<EVAL <LOCAL.TILES>>=<EVAL <TAG.CTF_TEAM>>
		LOCAL.FLAGS_AMOUNT_<EVAL <LOCAL.TILES>>=<EVAL <MORE1>>
ENDFOR
FORCLIENTS 500
	SYSMESSAGE @90 Final Countdown:
	FOR <EVAL <LOCAL.TILES>>
		SYSMESSAGE @<LOCAL.COLOR_<EVAL <LOCAL._FOR>>> Team <EVAL <LOCAL.TEAM_<EVAL <LOCAL._FOR>>>>: <EVAL <LOCAL.FLAGS_AMOUNT_<EVAL <LOCAL._FOR>>>>
