[FUNCTION spelluptimer]
FOR 16
	DOSWITCH <EVAL <LOCAL._FOR>-1>
		// Mage Spellup
		LOCAL.ID=i_rune_reactive_armor
		LOCAL.ID=i_rune_night_sight
		LOCAL.ID=i_rune_agility
		LOCAL.ID=i_rune_cunning
		LOCAL.ID=i_rune_protection
		LOCAL.ID=i_rune_strength
		LOCAL.ID=i_rune_bless
		LOCAL.ID=i_rune_incognito
		LOCAL.ID=i_rune_invisibility
		LOCAL.ID=i_spell_poly

		//Necro Spellup
		LOCAL.ID=m_nosferatu
		LOCAL.ID=m_bloodlust
		LOCAL.ID=m_vengeance

		// Bard Spellup
		LOCAL.ID=m_songintincrease 
		LOCAL.ID=m_songdexincrease
		LOCAL.ID=m_songstrincrease
	ENDDO

	IF (<SRC.FINDID.<LOCAL.ID>.UID>) && !(<SRC.FINDID.<LOCAL.ID>.LAYER>==44)
		IF (<EVAL <LOCAL._FOR>> < 11)
			IF !(<EVAL <LOCAL.MAGE_FOUND>>==1)
				SRC.SYSMESSAGE Mage Spellup:
			ENDIF
			LOCAL.MAGE_FOUND=1
			SRC.SYSMESSAGE <SRC.FINDID.<LOCAL.ID>.NAME>: <SPELL_UP_GET_TIMER <SRC.FINDID.<LOCAL.ID>.TIMER>>
		ELSEIF (<EVAL <LOCAL._FOR>> < 14)
			IF !(<EVAL <LOCAL.NECRO_FOUND>>==1)
				SRC.SYSMESSAGE Necromancy Spellup:
			ENDIF
			LOCAL.NECRO_FOUND=1
			SRC.SYSMESSAGE <SRC.FINDID.<LOCAL.ID>.NAME>: <SPELL_UP_GET_TIMER <SRC.FINDID.<LOCAL.ID>.TIMER>>
		ELSE	
			IF !(<EVAL <LOCAL.BARD_FOUND>>==1)
				SRC.SYSMESSAGE Bard Spellup:
			ENDIF
			LOCAL.BARD_FOUND=1
			IF (<EVAL <LOCAL._FOR>>==14)
				SRC.SYSMESSAGE INT Bard Song: <SPELL_UP_GET_TIMER <SRC.FINDID.<LOCAL.ID>.TIMER>>
			ELSEIF (<EVAL <LOCAL._FOR>>==15)
				SRC.SYSMESSAGE DEX Bard Song: <SPELL_UP_GET_TIMER <SRC.FINDID.<LOCAL.ID>.TIMER>>
			ELSEIF (<EVAL <LOCAL._FOR>>==16)
				SRC.SYSMESSAGE STR Bard Song: <SPELL_UP_GET_TIMER <SRC.FINDID.<LOCAL.ID>.TIMER>>
			ENDIF
		ENDIF
	ENDIF
ENDFOR

IF (<SRC.FINDLAYER.layer_flag_potion.UID>)
	LOCAL.POTION_FOUND=1
	SRC.SYSMESSAGE Potion Spellup:
	REF1 = <SRC.UID> 
	FORCHARLAYER 44
		REF1.SYSMESSAGE <NAME>: <SPELL_UP_GET_TIMER <TIMER>>
	ENDFOR
ENDIF

IF !(<EVAL <LOCAL.MAGE_FOUND>>==1) && !(<EVAL <LOCAL.NECRO_FOUND>>==1) && !(<EVAL <LOCAL.BARD_FOUND>>==1) && !(<EVAL <LOCAL.POTION_FOUND>>==1)
	SRC.SYSMESSAGE You dont appear to have any mage, necro, or bard spell up active.
ENDIF
RETURN 1

[FUNCTION spell_up_get_timer]
IF (<EVAL <ARGS>> > 300)
	LOCAL.SPELL_UP_TIMER=Over 5 minutes
ELSEIF (<EVAL <ARGS>> > 240)
	LOCAL.SPELL_UP_TIMER=4 minutes and <EVAL <ARGS>-240> seconds
ELSEIF (<EVAL <ARGS>> > 180)
	LOCAL.SPELL_UP_TIMER=3 minutes and <EVAL <ARGS>-180> seconds
ELSEIF (<EVAL <ARGS>> > 120)
	LOCAL.SPELL_UP_TIMER=2 minutes and <EVAL <ARGS>-120> seconds
ELSEIF (<EVAL <ARGS>> > 60)
	LOCAL.SPELL_UP_TIMER=1 minutes and <EVAL <ARGS>-60> seconds
ELSE
	LOCAL.SPELL_UP_TIMER=<EVAL <ARGS>> seconds
ENDIF
RETURN <LOCAL.SPELL_UP_TIMER>



