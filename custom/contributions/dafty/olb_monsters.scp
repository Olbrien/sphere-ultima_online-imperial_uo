//*********************************************************************************************************//
//                      Guild Town Traitor                                                                 //
//*********************************************************************************************************//

// .xtag.ENTERING_TIME= Set this to the Guild Town Traitor to set the hour at which the Guild Town can be invadaded (22 = 22pm, server time)
// .xtag.ENTERING_LOCATION= Set this to the Guild Town Traitor to set the location players will be teleported to when invading the Guild Town
// .xtag.ENTERING_HOUSE= Set this to the UID of the access pouch of the Guild Town Invaders House (to grant full access in that house to the invaders)
// .xtag.GUILD_ACCESS= Set this to the UID of the access pouch of the Guild Town ENTRY GATE 
// .xtag.GUILD_MAYOR= Set this to the UID of the Guild Town Mayore (check the Guild Town Entry Gate taglist (.xtaglist) and put here the same UID

[CHARDEF c_guildtown_traitor]
ID = c_man
NAME = Guild Town Traitor
TSPEECH = spk_guildtown_traitor

ON = @Create
STR= 10000
DEX= 10000
INT= 10000
INVUL
ITEMNEWBIE= random_male_hair
COLOR= colors_hair
ITEMNEWBIE= random_facial_hair
COLOR= match_hair
ITEMNEWBIE= i_CAPE
COLOR= 07a1
ITEMNEWBIE= i_robe
COLOR= 07a1

[SPEECH spk_guildtown_traitor]

ON=*hi*

IF !(<TAG0.ENTERING_TIME>) || !(<TAG0.ENTERING_LOCATION>) || !(<TAG0.ENTERING_HOUSE>) || !(<TAG0.GUILD_ACCESS>) || !(<TAG0.GUILD_MAYOR>)
	SAY I'm not yet properly configurated in order to be used.
	RETURN 1
ENDIF

IF (<SERV.RTIME.FORMAT %H>==<EVAL <TAG0.ENTERING_TIME>>)
	SAY You want to go inside this Guild Town? Say "Enter" and I'll smuggle you in.
ELSE
	SAY You want to go inside this Guild Town? Come back at <EVAL <TAG.ENTERING_TIME>> o'clock.
ENDIF

ON=*enter*

IF !(<TAG0.ENTERING_TIME>) || !(<TAG0.ENTERING_LOCATION>) || !(<TAG0.ENTERING_HOUSE>) || !(<TAG0.GUILD_ACCESS>) || !(<TAG0.GUILD_MAYOR>)
	SAY I'm not yet properly configurated in order to be used.
	RETURN 1
ENDIF

IF (<SRC.FINDID.m_guildtown_traitor_delay.UID>) && (STRMATCH("<SRC.FINDID.m_guildtown_traitor_delay.TAG0.GUILDTOWN_TRAITOR>","<UID>"))
	SAY You can't be smuggled inside this Guild Town for another <SRC.FINDID.m_guildtown_traitor_delay.TIMER> seconds.
	RETURN 1
ENDIF

LOCAL.SRC=<SRC.UID>

FORCONT <TAG.GUILD_ACCESS>
	IF (<MORE2>==<LOCAL.SRC>)
		SAY You don't seem to need to be smuggled into this Guild Town.
		RETURN 1
	ENDIF
ENDFOR

IF (STRMATCH("<TAG0.GUILD_MAYOR>","<SRC.UID>")
	SAY You don't seem to need to be smuggled into this Guild Town.
	RETURN 1
ENDIF

IF (<SERV.RTIME.FORMAT %H>==<EVAL <TAG0.ENTERING_TIME>>)
	EMOTE smuggling <SRC.NAME> into the Guild Town
	SRC.GO <TAG.ENTERING_LOCATION>
	SAY There you go, <SRC.NAME>
	SERV.NEWITEM = m_guildtown_traitor_delay
	NEW.TAG.GUILDTOWN_TRAITOR=<UID>
	NEW.TAG.ENTERING_HOUSE=<TAG.ENTERING_HOUSE>
	SRC.EQUIP <NEW.UID>
    SERV.NEWITEM = i_house_trust
    NEW.NAME = <SRC.NAME> (by <NAME>)
    NEW.MORE2 = <UID>
	NEW.MOREX = 1
    NEW.LINK = <SRC.UID>
    NEW.CONT = <TAG.ENTERING_HOUSE>
	SRC.SYSMESSAGE You have been smuggled into a Guild Town and cannot be again in the next hour. 
	SRC.SYSMESSAGE You have been granted full access to the Invaders House.
ELSE
	SAY I can not put you in at this time, please come back at <EVAL <TAG.ENTERING_TIME>> o'clock.
ENDIF


