[FUNCTION topservergold]

FORPLAYERS 6944
    IF !(<ISGM>)    
	
		LOCAL.GOLDCOUNT=<EVAL <LOCAL.GOLDCOUNT> + <GOLD>>
		
		LOCAL.GOLDCOUNTCHAR=<EVAL <LOCAL.GOLDCOUNTCHAR>+1>
		
		IF (<GOLD> > <EVAL <LOCAL.GOLDRECORD1>>)
		
			LOCAL.GOLDRECORD5=<LOCAL.GOLDRECORD4>
			LOCAL.GOLDRECORD5NAME=<LOCAL.GOLDRECORD4NAME>
			
			LOCAL.GOLDRECORD4=<LOCAL.GOLDRECORD3>
			LOCAL.GOLDRECORD4NAME=<LOCAL.GOLDRECORD3NAME>
			
			LOCAL.GOLDRECORD3=<LOCAL.GOLDRECORD2>
			LOCAL.GOLDRECORD3NAME=<LOCAL.GOLDRECORD2NAME>
			
			LOCAL.GOLDRECORD2=<LOCAL.GOLDRECORD1>
			LOCAL.GOLDRECORD2NAME=<LOCAL.GOLDRECORD1NAME>
			
			LOCAL.GOLDRECORD1= <GOLD>
			LOCAL.GOLDRECORD1NAME=<NAME>	
	
		ELSEIF (<GOLD> > <EVAL <LOCAL.GOLDRECORD2>>)
		
			LOCAL.GOLDRECORD5=<LOCAL.GOLDRECORD4>
			LOCAL.GOLDRECORD5NAME=<LOCAL.GOLDRECORD4NAME>
			
			LOCAL.GOLDRECORD4=<LOCAL.GOLDRECORD3>
			LOCAL.GOLDRECORD4NAME=<LOCAL.GOLDRECORD3NAME>
			
			LOCAL.GOLDRECORD3=<LOCAL.GOLDRECORD2>
			LOCAL.GOLDRECORD3NAME=<LOCAL.GOLDRECORD2NAME>
			
			LOCAL.GOLDRECORD2= <GOLD>
			LOCAL.GOLDRECORD2NAME=<NAME>
			
		ELSEIF (<GOLD> > <EVAL <LOCAL.GOLDRECORD3>>)
		
			LOCAL.GOLDRECORD5=<LOCAL.GOLDRECORD4>
			LOCAL.GOLDRECORD5NAME=<LOCAL.GOLDRECORD4NAME>
			
			LOCAL.GOLDRECORD4=<LOCAL.GOLDRECORD3>
			LOCAL.GOLDRECORD4NAME=<LOCAL.GOLDRECORD3NAME>
			
			LOCAL.GOLDRECORD3= <GOLD>
			LOCAL.GOLDRECORD3NAME=<NAME>
		
		ELSEIF (<GOLD> > <EVAL <LOCAL.GOLDRECORD4>>)
		
			LOCAL.GOLDRECORD5=<LOCAL.GOLDRECORD4>
			LOCAL.GOLDRECORD5NAME=<LOCAL.GOLDRECORD4NAME>
			
			LOCAL.GOLDRECORD4= <GOLD>
			LOCAL.GOLDRECORD4NAME=<NAME>
			
		ELSEIF (<GOLD> > <EVAL <LOCAL.GOLDRECORD4>>)
		
			LOCAL.GOLDRECORD5= <GOLD>
			LOCAL.GOLDRECORD5NAME=<NAME>
			
		ENDIF
		
	ENDIF
ENDFOR
		

SRC.SYSMESSAGE 1st - <LOCAL.GOLDRECORD1NAME> with <EVAL <LOCAL.GOLDRECORD1>>.
SRC.SYSMESSAGE 2nd - <LOCAL.GOLDRECORD2NAME> with <EVAL <LOCAL.GOLDRECORD2>>.
SRC.SYSMESSAGE 3rd - <LOCAL.GOLDRECORD3NAME> with <EVAL <LOCAL.GOLDRECORD3>>.
SRC.SYSMESSAGE 4th - <LOCAL.GOLDRECORD4NAME> with <EVAL <LOCAL.GOLDRECORD4>>.
SRC.SYSMESSAGE 5th - <LOCAL.GOLDRECORD5NAME> with <EVAL <LOCAL.GOLDRECORD5>>.

