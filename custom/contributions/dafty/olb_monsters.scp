	FOR <EVAL <TAG.DQ_MONSTERS_PICKED>>
		IF (STRMATCH("<TAG.DQ_MONSTER_ID_<EVAL <LOCAL._FOR>>>","<LOCAL.MONSTER_ID>"))
			f_pick_daily_quest 1
			RETURN 1
		ENDIF
	ENDFOR
ENDIF

// If the randomly selected monster had not come up before yet, then we will assign it to the Daily Quest, based on the TAG.ROUND count

TAG.DQ_MONSTER_ID_<EVAL <TAG.ROUND>>=<LOCAL.MONSTER_ID>
TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <TAG.ROUND>>=<LOCAL.MONSTER_DEATHS_REQUIRED>
TAG.DQ_MONSTER_NAME_<EVAL <TAG.ROUND>>=<LOCAL.MONSTER_NAME>
TAG.DQ_MONSTERS_PICKED=<EVAL <TAG0.DQ_MONSTERS_PICKED>+1>

IF (<EVAL <TAG0.ROUND>>==<EVAL <DEF.daily_quest_monsters_amount>>) // After picking the desired number of monsters, the function will stop searching for more. Until then, it will force itself again to selected another one.
	FORPLAYERS 6144
	    TAG.DQ_COMPLETED=
		f_clean_daily_quest
	ENDFOR
	SERV.LOG :DAILY QUEST: New Daily Quest started (<EVAL <MOREZ>>). Assigned monsters:
	
