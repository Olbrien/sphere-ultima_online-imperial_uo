[FUNCTION f_shopprize_reward]
IF (<SRC.RESTEST <ARGV[3]> i_pvptoken>)
	SRC.CONSUME <ARGV[3]> i_pvptoken
	SERV.NEWITEM <ARGV[1]>, <ARGV[2]>, <SRC.FINDLAYER.layer_bankbox.UID>
	SRC.SYSMESSAGE @07a1 You have sucessfully bought <ARGV[0]> for <EVAL <ARGV[3]>> PvP Tickets. You can find it in your bank!
ELSE
	SRC.SYSMESSAGE @07a1 "You do not have enough PvP Tickets for this item"
ENDIF
SRC.DIALOG d_ShopPrize, <EVAL <ARGV[4]>+1>
RETURN 1



