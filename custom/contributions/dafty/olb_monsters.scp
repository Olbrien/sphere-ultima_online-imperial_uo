[FUNCTION f_daily_quest_corpse]
OBJ=<UID.<ARGO.UID>.UID>
IF (<EVAL <OBJ.TAG0.COLECTED>>)
	SRC.SYSMESSAGE @07a1 This corpse has already been collected.
	RETURN 1
ENDIF

IF (<OBJ.BASEID>==i_corpse) && (<OBJ.LINK>==04fffffff)
	FOR <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTERS_PICKED>>
		LOCAL.MONSTER_KILLED_NAME=<UID.daily_quest_clock_uid.TAG.DQ_MONSTER_NAME_<EVAL <LOCAL._FOR>>>
		LOCAL.MONSTER_DEATHS_REQUIRED=<EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>>
		IF (<OBJ.TAG.BASEID>==<UID.daily_quest_clock_uid.TAG0.DQ_MONSTER_ID_<EVAL <LOCAL._FOR>>>)
			OBJ.TAG.COLECTED=1
			LOCAL.MONSTER=1
			IF (<EVAL <SRC.TAG0.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>> < <EVAL <UID.daily_quest_clock_uid.TAG0.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>>)
				SRC.TAG.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>=<EVAL <SRC.TAG0.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>+1>
				IF (<EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>-<SRC.TAG.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>> > 0)
					SRC.SYSMESSAGE @0a18 You have collected one <LOCAL.MONSTER_KILLED_NAME>. You need to collect <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>-<SRC.TAG.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>> more for the Daily Quest.
				ELSE
					FOR <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTERS_PICKED>>
						IF (<EVAL <SRC.TAG0.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>>==<EVAL <UID.daily_quest_clock_uid.TAG0.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>>)
							LOCAL.MONSTERS_KILLED=<EVAL <LOCAL.MONSTERS_KILLED>+1>
						ENDIF
					ENDFOR
					IF (<EVAL <LOCAL.MONSTERS_KILLED>>==<EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTERS_PICKED>>)
						SRC.TAG.DQ_COMPLETED=<EVAL <UID.daily_quest_clock_uid.MOREZ>>
						SERV.NEWITEM i_crate_small, 1, <SRC.FINDLAYER.layer_bankbox.UID>
						NEW.COLOR=01f2
						NEW.NAME=@01f2 Daily Hunting Quest Reward
						LOCAL.NEWUID=<NEW.UID>
						SERV.NEWITEM i_pvptoken, 5, <LOCAL.NEWUID>
						SRC.SYSMESSAGE @07a1 You've completed your Daily Quest. The reward has been placed at your bankbox.
						SERV.ALLCLIENTS SYSMESSAGE @00624 <SRC.NAME> just completed the Daily Quest!
						DISCORD_SEND 1 <SRC.NAME> just completed the Daily Quest!
						SRC.f_clean_daily_quest
						SERV.LOG :DAILY QUEST: <SRC.UID> <SRC.ACCOUNT> <SRC.NAME> completed a Daily Quest (<EVAL <UID.daily_quest_clock_uid.MOREZ>>) <EVAL (23-<UID.daily_quest_clock_uid.MOREY>)> hours, <EVAL (59-<UID.daily_quest_clock_uid.MOREX>)> minutes and <UID.daily_quest_clock_uid.TIMER> before a new Dialy Quest starts.
					ELSE
						IF !(<EVAL <LOCAL.FOUND>>==1)
							SRC.SYSMESSAGE @0a18 You have killed the required <EVAL <LOCAL.MONSTER_DEATHS_REQUIRED>> <LOCAL.MONSTER_KILLED_NAME>'s.
							SRC.SYSMESSAGE @07a1 You have <EVAL (23-<UID.daily_quest_clock_uid.MOREY>)> hours, <EVAL (59-<UID.daily_quest_clock_uid.MOREX>)> minutes and <UID.daily_quest_clock_uid.TIMER> seconds to hunt:
							FOR <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTERS_PICKED>>
								SRC.SYSMESSAGE @0a18 <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>-<SRC.TAG.DQ_MONSTER_KILLED_<EVAL <LOCAL._FOR>>>> out of <EVAL <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_DEATHS_REQUIRED_<EVAL <LOCAL._FOR>>>> <UID.daily_quest_clock_uid.TAG.DQ_MONSTER_NAME_<EVAL <LOCAL._FOR>>>'s
							ENDFOR
							LOCAL.FOUND=1
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDFOR
	IF !(<EVAL <LOCAL.MONSTER>>==1)
		SRC.SYSMESSAGE @07a1 This doesnt seem to be a monster corpse you need for the Daily Quest.
		RETURN 1
	ENDIF
ELSE
	SRC.SYSMESSAGE @07a1 You must target a monster corpse with this function in order to collect it.
	RETURN 1
ENDIF


