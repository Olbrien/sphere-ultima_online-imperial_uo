[FUNCTION f_clear_stored_ip]
FOR <EVAL <TAG.NUMBER_OF_STORED_IPS>>
    IF (STRMATCH("<ARGS>","<TAG.STORED_IP_<EVAL <LOCAL._FOR>>>"))
        TAG.STORED_IP_<EVAL <LOCAL._FOR>>=
        LOCAL.TEMP=<EVAL <LOCAL._FOR>>
        FOR <EVAL <LOCAL.TEMP>> <EVAL <TAG.NUMBER_OF_STORED_IPS>>
            TAG.STORED_IP_<EVAL <LOCAL._FOR>>=<TAG.STORED_IP_<EVAL <LOCAL._FOR>+1>>
            TAG.STORED_IP_<EVAL <LOCAL._FOR>+1>=   
            TAG.STORED_IP_<EVAL <LOCAL._FOR>>_ACCOUNT=<TAG.STORED_IP_<EVAL <LOCAL._FOR>+1>_ACCOUNT>
            TAG.STORED_IP_<EVAL <LOCAL._FOR>+1>_ACCOUNT=
        ENDFOR
        TAG.NUMBER_OF_STORED_IPS=<EVAL <TAG0.NUMBER_OF_STORED_IPS>-1>
        RETURN 1
    ENDIF
ENDFOR
RETURN 0

