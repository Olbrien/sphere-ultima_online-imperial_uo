	LOCAL.SKILL=<SERV.SKILL.<EVAL (<LOCAL._FOR>-1)>.KEY>
	LOCAL.SKILLAMOUNT=<EVAL <SRC.<LOCAL.SKILL>>>
	IF (<EVAL <LOCAL.SKILLAMOUNT>> > 0)
		SRC.TAG.SKILLROBE<EVAL (<LOCAL._FOR>-1)>=<LOCAL.SKILLAMOUNT>
	ENDIF
ENDFOR

SRC.ALLSKILLS=0

// Load skills from the Temporary Skills Robe

FOR 1 48
	IF (<EVAL <TAG0.SKILL<EVAL (<LOCAL._FOR>-1)>>> > 0)
		LOCAL.SKILL=<SERV.SKILL.<EVAL (<LOCAL._FOR>-1)>.KEY>
		SRC.<LOCAL.SKILL>=<EVAL <TAG.SKILL<EVAL (<LOCAL._FOR>-1)>>>
	ENDIF
ENDFOR

ON=@UNEQUIP

// Remove all player skills

SRC.ALLSKILLS=0

// Retrieve player skills previously saved from TAG's

FOR 1 48
	IF (<EVAL <SRC.TAG0.SKILLROBE<EVAL (<LOCAL._FOR>-1)>>> > 0)
		LOCAL.SKILL=<SERV.SKILL.<EVAL (<LOCAL._FOR>-1)>.KEY>
		SRC.<LOCAL.SKILL>=<EVAL <SRC.TAG.SKILLROBE<EVAL (<LOCAL._FOR>-1)>>>
		SRC.TAG.SKILLROBE<EVAL (<LOCAL._FOR>-1)>=
	ENDIF
ENDFOR

