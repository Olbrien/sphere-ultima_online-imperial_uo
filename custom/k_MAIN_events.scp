[ITEMDEF m_travelstone_delay]
NAME=Travel Stone Delay
ID=i_handr_1
TYPE=T_EQ_SCRIPT
WEIGHT=0
LAYER=layer_special
 
ON=@TIMER
SERV.LOG :TRAVEL STONE: <CONT.UID> <CONT.ACCOUNT> <CONT.NAME> is going from <CONT.P> to <MOREP> through the Travel Stone.
CONT.SOUND=snd_spell_recall
CONT.GO <MOREP>
CONT.EVENTS -e_travelstone_break   
REMOVE
RETURN 1	


[FUNCTION f_sendpacket_orion]
//Removed Changing Trees to Stumps, Marking Cave Tiles, No Vegetation, No Draw Roofs, No Change Fields to Tiles, Boxhack, Available Dark Nights, Clearing the Way, Auto Open Doors
SENDPACKET B0BF W000B W0FACE W0032 D0FFFF38B1
// Removed Light Filter, No Death Screen, AutoStart, Corpses Autoopen, Stealth Steps Counter, Tracker, Ignore Stamina Check, Reconnector, Autoloot and Fast Rotation
SENDPACKET B0BF W017 W0FACE W08001 D0FFFFFFFF D0FAFF96B4 D0FFFFFFFF D0FFFFFFFF
// Crash Injection Clients and others
SENDPACKET B0B0 W0020 D00000001 D00000409 D00000046 D00000046 D00037B7D D000001FF D0FF000000 B00
//Packet that will demand the client version. In case of wrong version it flushes the client.
//To make it work add to the sphere.ini PACKET191=f_orion_exclusive_sphere_ini (it's what it will block the client)
SENDPACKET B0BF W0007 W0FACE W0034


[FUNCTION f_orionexclusive]
if (<tag0.iwillkickyou>==1)
  serv.log WARNING Player is using a client different than Orion UO
  Sysmessage This shard require ORION client
  DISCONNECT_NOW
ENDIF

