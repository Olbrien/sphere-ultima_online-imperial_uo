f_spell_check_travelstone <ARGN>
IF (<VAR.f_spell_check_travelstone>)
    CONSUME m_travelstone_delay
    EVENTS -e_travelstone_break
    MESSAGE Travel Cancelled.
    RETURN 0
ENDIF
 
ON=@GetHit
CONSUME m_travelstone_delay
EVENTS -e_travelstone_break
MESSAGE Travel Cancelled.
RETURN 0
   
