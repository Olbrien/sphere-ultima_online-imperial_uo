on = @SkillStart		// trigger for the skill progress system
   SRC.DEBUGSAY SkillStart <ACTION>
   
	IF !(<SRC.FINDID(i_afk_check_delay)>) // AFK Check won't appear again for 60 minutes after successfully answering to one

	IF ((<ACTION> == skill_mining) && (RAND(100) <= 5)) // While Mining, 5% chance to get AFK Checked
		IF !(strmatch("<REGION.NAME>","Mt Kendall")) // Unless the player is at Mt Kendall he gets AFK Checked regardless of skill amount or lock status
			SRC.AUTOMATICAFKCHECK 
		ELSE
			IF !( <SRC.SKILLLOCK.45> == 0 ) // If the player is at Mt Kendall but not sparring the skill (Arrow not up) he gets AFK Checked
				SRC.AUTOMATICAFKCHECK 
			ELSE
				IF !(<SRC.MINING> < 1000) // If the player is at Mt Kendall with arrow up but with more or equal to 100% Mining he gets AFK Checked
					SRC.AUTOMATICAFKCHECK
				ENDIF
			ENDIF
		ENDIF
	
	ELSEIF ((<ACTION> == skill_lumberjack) && (RAND(100) <= 5)) // While Lumbering, 5% chance to get AFK Checked		
		IF !(<SRC.SKILLLOCK.44>==0) // If skill arrow is not up he gets AFK Checked
			SRC.AUTOMATICAFKCHECK
		ELSE
			IF !(<SRC.LUMBERJACKING> < 1000) // If the arrow is up but Lumberjacking is equal or higher than 100% he gets AFK Checked
				SRC.AUTOMATICAFKCHECK
			ENDIF
		ENDIF
	
	ELSEIF ((<ACTION> == skill_fishing) && (RAND(100) <= 5)) // While Fishing, 5% chance to get AFK Checked	
		IF !(<SRC.SKILLLOCK.18>==0) // If skill arrow is not up he gets AFK Checked
			SRC.AUTOMATICAFKCHECK
		ELSE
			IF !(<SRC.FISHING> < 1000) // If the arrow is up but Fishing is equal or higher than 100% he gets AFK Checked
				SRC.AUTOMATICAFKCHECK
			ENDIF
		ENDIF
		
	ENDIF
			
	ENDIF
   
   //If the skill that is gonna try to roll isn't the skill fix it
   If !(<ACTION> = <SRC.findlayer(layer_k_control).MORE>)
      SRC.findlayer(layer_k_control).MORE=<ACTION>
   ENDIF
   
   //Don't run on npcs
   if ( <SRC.ISNPC> )
      RETURN 0
   endif
  
   //See if they're paralyzed
   if ( <FLAGS> & statf_freeze )
      SYSMESSAGE You can't do much in your current state.
      return 1
   endif
   
   IF ((<eval (<flags>&02000)==02000>) && (<ACCOUNT.PLEVEL> <= 1))
   if !(<FINDID.i_skargard_phased.UID>)
      SYSMESSAGE You cannot accomplish this in your current form.
      RETURN 1
   endif
   ENDIF

   //Make sure this is a valid skill number?
   if ( <eval (<SERV.LOOKUPSKILL <SERV.SKILL(<ACTION>).KEY>>)> > 53 )
      // src.sysmessage Action <action> is not raisable.
      RETURN 0
   endif


   VAR.RETURN		= 0
   VAR.NOIMPROVE	= 0

   SRC.TAG.SKILLSTART.x		= <SRC.P.x>
   SRC.TAG.SKILLSTART.y		= <SRC.P.y>

   if ( 0<SRC.TAG.LastTarg> == 0 )
      SRC.TAG.LastTarg	= 0
   endif
   
   
   if ( 0 )
   elseif ( <action> == Skill_Unused1 )
      return 0
   elseif ( <action>  == Skill_Unused2 ) 
      return 0
   elseif ( <action> == skill_stealing )
      IF (<SRC.MEMORYFINDTYPE(memory_fight).uid>)
         SRC.SYSMESSAGE You cannot fight whilst in combat!
         RETURN 1
      ELSEIF (<SRC.FLAGS> & statf_war)
         SRC.SYSMESSAGE You cannot steal whilst in warmode!
         RETURN 1
      ELSEIF (<SRC.ACCOUNT.JAIL>=1)
         SRC.SYSMESSAGE Stealing whilst in Jail... Not a good idea.
         RETURN 1
      ELSEIF ((<ACT.TOPOBJ.ID>==c_h_player_vendor) || (<ACT.TOPOBJ.ID>==c_h_player_vendor_f))
         SRC.SYSMESSAGE Player vendors are trained in keeping thieves away...
         RETURN 1
      ELSEIF (<ACT.TOPOBJ.FLAGS> & statf_invul )
         SRC.SYSMESSAGE You cannot steal from an immortal...
         RETURN 1
      ELSEIF ((<ACT.TOPOBJ.MEMORYFINDTYPE(memory_fight).uid>) && (<ACT.TOPOBJ.ISPLAYER>))
         SRC.SYSMESSAGE You cannot steal from someone who is fighting!
         RETURN 1
      ELSEIF (<ACT.TOPOBJ.ISPLAYER>)
         SRC.SYSMESSAGE You cannot steal from players at this time...
         SKILLINIT_stop
         RETURN 1
      ENDIF
      
      LOCAL.STEALSUCCESS=<EVAL RAND(80)> + (<EVAL <SRC.STEALING> / 50) + 1>
     
      IF (<EVAL <LOCAL.STEALSUCCESS>> >= 95)
         SRC.SYSMESSAGE You begin to rummage through <ACT.TOPOBJ.NAME>'s belongings with confidence, and gain fame for it...
         IF (<SRC.FAME> < 9990)
            SRC.FAME=<SRC.FAME>+10
         ENDIF
      ELSEIF (<EVAL <LOCAL.STEALSUCCESS>> >= 75)
         SRC.SYSMESSAGE You begin to rummage through <ACT.TOPOBJ.NAME>'s belongings 
      ELSEIF (<EVAL <LOCAL.STEALSUCCESS>> >= 55)
         SRC.SYSMESSAGE You go to rummage through <ACT.TOPOBJ.NAME>'s backpack but struggle to grasp anything...
         RETURN 1
      ELSEIF (<EVAL <LOCAL.STEALSUCCESS>> >= 30)
         IF ((<ACT.TOPOBJ.KARMA> > 500) || (<ACT.TOPOBJ.KILLS> <= 5))
            SRC.CRIMINAL
         ENDIF
         SRC.SYSMESSAGE You fumble through <ACT.TOPOBJ.NAME>'s belongings desperately as <ACT.TOPOBJ.SEX he/she> catches you out!
         SRC.SAY <SRC.NAME> is attempting to steal from <ACT.TOPOBJ.NAME>!
      ELSE
         IF ((<ACT.TOPOBJ.KARMA> > -900 || (<ACT.TOPOBJ.KILLS> <= 5))
            SRC.CRIMINAL
         ENDIF
         SRC.SYSMESSAGE You go to rummage through <ACT.TOPOBJ.NAME>'s backpack but <ACT.TOPOBJ.SEX he/she> stops you, and you lose fame for it!
         SRC.SAY <SRC.NAME> is attempting to steal from <ACT.TOPOBJ.NAME>!
         IF (<SRC.FAME> > 10)
            SRC.FAME=<SRC.FAME>-10
         ENDIF
         RETURN 1
      ENDIF
      
//      IF !(<CONT.LAYER>==layer_backpack)
//         SRC.SYSMESSAGE You can only steal by clicking a player or NPC!
//         RETURN 1
//      ENDIF
   elseif ( <action> == Skill_Magery )
      SRC.TAG.LASTSPELL	= <actarg1>
      return 0
   endif
   

   //Send through custom init checks
   SAFETRY SRC.SKILLINIT_<eval (<SERV.LOOKUPSKILL <SERV.SKILL(<ACTION>).KEY>>)>
   if ( <var.return> != 0 )
      ACTION = -1
      return 1
   endif


   if ( 0 )

   //Disable preset skills
   
//   elseif ( <action> == Skill_Anatomy)
//      SRC.SYSMESSAGE Assessment is disabled
//      return 1
//   elseif ( <action> == Skill_EvalInt)
//      SRC.SYSMESSAGE Mystic Lore is disabled
//      return 1
//   elseif ( <action> == Skill_AnimalLore)
//      SRC.SYSMESSAGE Assessment is disabled at this time...
//      RETURN 1
   //Pass Through Resource Gathering skills to avoid rolls on "no resource" messages
   elseif ( <action> == Skill_Fishing )
      return 0
   elseif ( <action> == Skill_Mining )
      return 0
   elseif ( <action> == Skill_Lumberjack )
      return 0
      


   //Pass Through Combats skills...not sure why we were doin this...maybe remove this
   elseif ( <action> == Skill_Archery )
      return 0
   elseif ( <action> == Skill_Wrestling )
      return 0
   elseif ( <action> == Skill_Macefighting )
      return 0
   elseif ( <action> == Skill_Swordsmanship )
      return 0
   elseif ( <action> == Skill_Fencing )
      return 0
   elseif ( <action> == Skill_Tactics )
      return 0

   //Pass Through Lores to run action on success
   elseif ( <action> == Skill_ArmsLore )
      return 0
   elseif ( <action> == Skill_AnimalLore )
      return 0
   elseif ( <action> == Skill_ItemLore )
      if ( 0<SRC.ACT.CONT> )
         //Alteration here
         if ( <SRC.ACT.TYPE> == t_container )
            action = -1
            SRC.ACT.INSPECT_BAG
            return 1
         endif
         //Alteration here
      endif
      return 0
   elseif ( <action> == Skill_NetherLore )
      return 0
      
   //Dunno why we're passing this one either - maybe remove this
